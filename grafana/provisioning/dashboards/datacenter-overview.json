          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n  time_bucket('30 seconds', timestamp) AS time,\n  server_id || '_rx' as metric,\n  AVG(network_rx_mbps) as value\nFROM server_metrics\nWHERE $__timeFilter(timestamp)\nGROUP BY time, server_id\nUNION ALL\nSELECT \n  time_bucket('30 seconds', timestamp) AS time,\n  server_id || '_tx' as metric,\n  AVG(network_tx_mbps) as value\nFROM server_metrics\nWHERE $__timeFilter(timestamp)\nGROUP BY time, server_id\nORDER BY time",
